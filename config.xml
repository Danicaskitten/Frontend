<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dsdproject.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Movie Bot</name>
    <description>
       A cross-platform mobile application created for the DSD course.
    </description>
    <author>
        Movie Bot Team 
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <engine name="browser" spec="~4.1.0" />
    <engine name="android" spec="~6.0.0" />
</widget>
